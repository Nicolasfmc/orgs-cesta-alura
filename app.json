{
  "expo": {
    "name": "SislogWeb App",
    "slug": "orgs-cesta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoQ.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashGrazz.png",
      "resizeMode": "contain",
      "backgroundColor": "#00b2a6"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
